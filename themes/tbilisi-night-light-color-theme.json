{
  "$schema": "vscode://schemas/color-theme",
  "name": "Tbilisi Night Light",
  "type": "light",
  "colors": {
    "editor.background": "#faf8fc",
    "editor.foreground": "#2d1f3d",
    "editorLineNumber.foreground": "#b8a8d0",
    "editorLineNumber.activeForeground": "#6b4c8f",
    "editorCursor.foreground": "#ff8c42",
    "editor.selectionBackground": "#e8d4f5",
    "editor.selectionHighlightBackground": "#f0e5fa80",
    "editor.lineHighlightBackground": "#f5f0ff",
    "editorIndentGuide.background1": "#e8d4f5",
    "editorIndentGuide.activeBackground1": "#d4b8e8",
    "editorWhitespace.foreground": "#e8d4f5",
    "editorWidget.background": "#ffffff",
    "editorWidget.border": "#d4b8e8",
    "editorSuggestWidget.background": "#ffffff",
    "editorSuggestWidget.border": "#d4b8e8",
    "editorSuggestWidget.foreground": "#2d1f3d",
    "editorSuggestWidget.highlightForeground": "#ff8c42",
    "editorSuggestWidget.selectedBackground": "#f0e5fa",
    "input.background": "#ffffff",
    "input.border": "#d4b8e8",
    "input.foreground": "#2d1f3d",
    "inputOption.activeBorder": "#ff8c42",
    "list.activeSelectionBackground": "#e8d4f5",
    "list.activeSelectionForeground": "#8b4c7a",
    "list.inactiveSelectionBackground": "#f5f0ff",
    "list.hoverBackground": "#f5f0ff",
    "list.focusBackground": "#e8d4f5",
    "sideBar.background": "#f5f0ff",
    "sideBar.foreground": "#4a3d5a",
    "sideBarTitle.foreground": "#8b4c7a",
    "sideBarSectionHeader.background": "#f5f0ff",
    "sideBarSectionHeader.foreground": "#6b4c8f",
    "activityBar.background": "#faf8fc",
    "activityBar.foreground": "#8b7a9f",
    "activityBar.activeBorder": "#ff8c42",
    "activityBarBadge.background": "#ff8c42",
    "activityBarBadge.foreground": "#ffffff",
    "statusBar.background": "#faf8fc",
    "statusBar.foreground": "#6b4c8f",
    "statusBar.noFolderBackground": "#faf8fc",
    "statusBar.debuggingBackground": "#b88bc7",
    "statusBarItem.hoverBackground": "#f0e5fa",
    "statusBarItem.activeBackground": "#e8d4f5",
    "titleBar.activeBackground": "#faf8fc",
    "titleBar.activeForeground": "#2d1f3d",
    "titleBar.inactiveBackground": "#faf8fc",
    "titleBar.inactiveForeground": "#8b7a9f",
    "tab.activeBackground": "#ffffff",
    "tab.activeForeground": "#8b4c7a",
    "tab.inactiveBackground": "#f5f0ff",
    "tab.inactiveForeground": "#8b7a9f",
    "tab.border": "#faf8fc",
    "tab.activeBorder": "#ff8c42",
    "tab.hoverBackground": "#faf8fc",
    "panel.background": "#faf8fc",
    "panel.border": "#d4b8e8",
    "panelTitle.activeForeground": "#8b4c7a",
    "panelTitle.inactiveForeground": "#8b7a9f",
    "terminal.background": "#faf8fc",
    "terminal.foreground": "#2d1f3d",
    "terminal.ansiBlack": "#faf8fc",
    "terminal.ansiRed": "#d85a5a",
    "terminal.ansiGreen": "#5fa069",
    "terminal.ansiYellow": "#d4a84f",
    "terminal.ansiBlue": "#4b7bb1",
    "terminal.ansiMagenta": "#a86bc7",
    "terminal.ansiCyan": "#3fa3a3",
    "terminal.ansiWhite": "#2d1f3d",
    "terminal.ansiBrightBlack": "#b8a8d0",
    "terminal.ansiBrightRed": "#e85a5a",
    "terminal.ansiBrightGreen": "#6fb069",
    "terminal.ansiBrightYellow": "#f4b866",
    "terminal.ansiBrightBlue": "#5b8bc1",
    "terminal.ansiBrightMagenta": "#b88bc7",
    "terminal.ansiBrightCyan": "#4fb3b3",
    "terminal.ansiBrightWhite": "#3d2f4d",
    "button.background": "#ff8c42",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#ff9c5a",
    "badge.background": "#ff8c42",
    "badge.foreground": "#ffffff",
    "scrollbarSlider.background": "#d4b8e880",
    "scrollbarSlider.hoverBackground": "#c4a8d880",
    "scrollbarSlider.activeBackground": "#b498c880",
    "progressBar.background": "#ff8c42",
    "dropdown.background": "#ffffff",
    "dropdown.border": "#d4b8e8",
    "dropdown.foreground": "#2d1f3d",
    "checkbox.background": "#ffffff",
    "checkbox.border": "#d4b8e8",
    "textLink.foreground": "#4b7bb1",
    "textLink.activeForeground": "#3b6ba1",
    "textBlockQuote.background": "#f5f0ff",
    "textBlockQuote.border": "#d4b8e8",
    "textCodeBlock.background": "#f5f0ff",
    "textPreformat.foreground": "#8b4c7a",
    "editorBracketMatch.background": "#f0e5fa",
    "editorBracketMatch.border": "#ff8c42",
    "editorError.foreground": "#d85a5a",
    "editorWarning.foreground": "#d4a84f",
    "editorInfo.foreground": "#4b7bb1",
    "editorHint.foreground": "#3fa3a3",
    "editorGutter.background": "#faf8fc",
    "editorGutter.modifiedBackground": "#d4a84f",
    "editorGutter.addedBackground": "#5fa069",
    "editorGutter.deletedBackground": "#d85a5a",
    "diffEditor.insertedTextBackground": "#5fa06920",
    "diffEditor.removedTextBackground": "#d85a5a20",
    "merge.currentHeaderBackground": "#4b7bb140",
    "merge.incomingHeaderBackground": "#5fa06940",
    "merge.commonHeaderBackground": "#d4b8e840",
    "peekView.border": "#ff8c42",
    "peekViewEditor.background": "#ffffff",
    "peekViewResult.background": "#f5f0ff",
    "peekViewResult.selectionBackground": "#e8d4f5",
    "breadcrumb.foreground": "#8b7a9f",
    "breadcrumb.focusForeground": "#2d1f3d",
    "breadcrumb.activeSelectionForeground": "#8b4c7a",
    "breadcrumbPicker.background": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8b7a9f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#5fa069"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#d4a84f"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#8b4c7a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function"
      ],
      "settings": {
        "foreground": "#4b7bb1"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#8b4c7a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#2d1f3d"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type"
      ],
      "settings": {
        "foreground": "#3fa3a3"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#a86bc7"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#d85a5a"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d4a84f"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.other",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#6b4c8f"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#3fa3a3"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#8b4c7a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#8b4c7a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#4b7bb1",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#8b4c7a"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#4b7bb1"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#d85a5a"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#5fa069"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d85a5a"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#d4a84f",
        "fontStyle": "strikethrough"
      }
    }
  ]
}

